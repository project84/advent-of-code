	"bugs": {
		"url": "https://github.com/project84/advent-of-code-21/issues"
	},
	"homepage": "https://github.com/project84/advent-of-code-21#readme",
	"devDependencies": {
		"@babel/cli": "^7.16.0",
		"@babel/core": "^7.16.0",
		"@babel/node": "^7.16.0",
		"@babel/preset-env": "^7.16.4",
		"js-md5": "^0.7.3",
		"readline-sync": "^1.4.10"
	},
	"dependencies": {
		"minimist": "^1.2.5"
	}
